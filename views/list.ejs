<%- include("header") -%> 
    <div class="box" id="heading">
        <h1><%= dayname %></h1>
    </div>
    <div class="box">
        <% listItems.forEach(element => { %>
            <div class="item">
                <input type="checkbox">
                <p><%= element %> </p>
            </div>
        <% }) %>
        
        <form class="item" action="/" method="post">
            <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
            <button type="submit" name="btnlist" value="<%= dayname %>">+</button>
        </form>
    </div>
<%- include("footer") -%> 